{
  "name": "@blacktokki/notebook",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "http://blacktokki.github.io/blacktokki-notebook",
  "scripts": {
    "build": "yarn --cwd ../../ workspace @blacktokki/editor tinymce ../../apps/notebook/public",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web --port 19006",
    "github": "expo export -p web && replace-in-file --configFile=scripts/seo.js && gh-pages -d web-build -r https://github.com/blacktokki/blacktokki-notebook",
    "pwa:icon": "npx expo-pwa icon --platform chrome -i assets/icon.png",
    "analyzer": "node scripts/analyzer.js && expo start --web --no-dev --minify && rimraf webpack.config.js",
    "script:md": "node scripts/script-to-md.js"
  },
  "dependencies": {
    "@blacktokki/core": "*",
    "@blacktokki/editor": "*",
    "@blacktokki/eslint-config": "*",
    "@blacktokki/navigation": "*",
    "@expo/metro-runtime": "~3.1.3",
    "@react-native-async-storage/async-storage": "1.21.0",
    "axios": "^1.8.2",
    "expo": "^50.0.0",
    "expo-linking": "~6.2.2",
    "expo-status-bar": "~1.11.1",
    "jszip": "^3.10.1",
    "react": "*",
    "react-dom": "<18.3",
    "react-native": "*",
    "react-native-paper": "^5.13.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-vector-icons": "*",
    "react-native-web": "~0.19.6",
    "react-query": "^3.39.2"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@expo/metro-config": "~0.17.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "gh-pages": "^5.0.0",
    "replace-in-file": "^7.0.1",
    "rimraf": "^5.0.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "eslintConfig": {
    "extends": "@blacktokki/eslint-config"
  },
  "private": true
}
