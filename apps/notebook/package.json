{
  "name": "@blacktokki/notebook",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "http://blacktokki.github.io/blacktokki-notebook",
  "scripts": {
    "build": "yarn --cwd ../../ workspace @blacktokki/editor tinymce ../../apps/notebook/web",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "github": "npx expo export:web && replace-in-file --configFile=scripts/seo.js && gh-pages -d web-build -r https://github.com/blacktokki/blacktokki-notebook",
    "pwa": "npx expo-pwa",
    "analyzer": "node scripts/analyzer.js && expo start --web --no-dev --minify && rimraf webpack.config.js"
  },
  "dependencies": {
    "@blacktokki/core": "*",
    "@blacktokki/editor": "*",
    "@blacktokki/eslint-config": "*",
    "@blacktokki/navigation": "*",
    "@dnd-kit/core": "6.0.8",
    "@dnd-kit/sortable": "7.0.2",
    "@dnd-kit/utilities": "3.2.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "axios": "^1.7.9",
    "expo": "^49.0.23",
    "expo-linking": "~5.0.2",
    "expo-status-bar": "~1.6.0",
    "react": "*",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "<18.3",
    "react-native": "*",
    "react-native-paper": "^5.13.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.6.3",
    "react-native-vector-icons": "*",
    "react-native-web": "~0.19.6",
    "react-query": "^3.39.2"
  },
  "devDependencies": {
    "@babel/core": "*",
    "@expo/webpack-config": "^19.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "gh-pages": "^3.2.2",
    "replace-in-file": "^7.0.1",
    "rimraf": "^5.0.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "eslintConfig": {
    "extends": "@blacktokki/eslint-config"
  },
  "private": true
}
